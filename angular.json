{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "samvloeberghs": {
      "root": "projects/samvloeberghs/",
      "sourceRoot": "projects/samvloeberghs/src",
      "projectType": "application",
      "prefix": "sv",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/samvloeberghs/browser",
            "index": "projects/samvloeberghs/src/index.html",
            "main": "projects/samvloeberghs/src/main.ts",
            "polyfills": "projects/samvloeberghs/src/polyfills.ts",
            "tsConfig": "projects/samvloeberghs/tsconfig.app.json",
            "assets": [
              "projects/samvloeberghs/src/favicon.ico",
              "projects/samvloeberghs/src/assets",
              "projects/samvloeberghs/src/manifest.json"
            ],
            "styles": [
              "projects/samvloeberghs/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/samvloeberghs/src/environments/environment.ts",
                  "with": "projects/samvloeberghs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                }
              ],
              "serviceWorker": true
            },
            "local": {
              "fileReplacements": [
                {
                  "replace": "projects/samvloeberghs/src/environments/environment.ts",
                  "with": "projects/samvloeberghs/src/environments/environment.local.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                }
              ],
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "samvloeberghs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "samvloeberghs:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "samvloeberghs:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/samvloeberghs/tsconfig.app.json",
              "projects/samvloeberghs/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/samvloeberghs/server",
            "main": "projects/samvloeberghs/src/main.server.ts",
            "tsConfig": "projects/samvloeberghs/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/samvloeberghs/src/environments/environment.ts",
                  "with": "projects/samvloeberghs/src/environments/environment.prod.ts"
                }
              ]
            },
            "local": {
              "fileReplacements": [
                {
                  "replace": "projects/samvloeberghs/src/environments/environment.ts",
                  "with": "projects/samvloeberghs/src/environments/environment.local.ts"
                }
              ]
            }
          }
        }
      }
    },
    "jsonld": {
      "root": "projects/jsonld",
      "sourceRoot": "projects/jsonld/src",
      "projectType": "library",
      "prefix": "sv-jsonld",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/jsonld/tsconfig.lib.json",
            "project": "projects/jsonld/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/jsonld/tsconfig.lib.json",
              "projects/jsonld/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "samvloeberghs"
}
